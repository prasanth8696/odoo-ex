<odoo>



	<record id="estate_property_type_form_eight" model="ir.ui.view">
		<field name="name">estate.property.type.eight.form</field>
		<field name="model">estate.property.type.eight</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="properties">
				<sheet>
						<div>
							 <h1 class="mb32">
                        	    <field name="name" class="mb16"/>
                        	 </h1>						
						</div>
				</sheet>
			</form>
		
		</field>
	</record>
	
	
	<record id="estate_property_type_eight_search" model="ir.ui.view">
		<field name="name">estate.property.type.eight.search</field>
		<field name="model">estate.property.type.eight</field>
		<field name="type" >search</field>
		<field name="arch" type="xml">
			<search>
				<field name="name" />
			</search>
		</field>
	
	</record>








	<record id="estate_property_type_eight_action" model= "ir.actions.act_window">
		<field name="name" >Settings</field>
		<field name="res_model">estate.property.type.eight</field>
		<field name="view_mode">tree,form</field>
	</record>
	



























</odoo>